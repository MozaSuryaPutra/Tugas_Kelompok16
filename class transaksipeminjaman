   private AnggotaPerpustakaan anggota;
   private Date tanggal;
   private int IdTransaksi;

   public int getIdTransaksi() {
      return IdTransaksi;
   }

   public void setIdTransaksi(int IdTransaksi) {
      this.IdTransaksi = IdTransaksi;
   }

